{
  "name": "sync-pkg",
  "description": "CLI to sync only basic properties from package.json to bower.json.",
  "version": "0.6.4",
  "homepage": "https://github.com/jonschlinkert/sync-pkg",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "contributors": [
    "Anders D. Johnson (https://github.com/adjohnson916)"
  ],
  "repository": "jonschlinkert/sync-pkg",
  "bugs": {
    "url": "https://github.com/jonschlinkert/sync-pkg/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "cli.js"
  ],
  "main": "index.js",
  "scripts": {
    "test": "mocha"
  },
  "preferGlobal": true,
  "bin": {
    "sync-pkg": "cli.js"
  },
  "dependencies": {
    "ansi-green": "^0.1.1",
    "array-unique": "^0.2.1",
    "arrayify-compact": "^0.2.0",
    "extend-shallow": "^2.0.1",
    "filter-object": "^2.1.0",
    "kind-of": "^2.0.1",
    "load-pkg": "^2.0.1",
    "minimist": "^1.2.0",
    "omit-empty": "^0.3.1",
    "success-symbol": "^0.1.0",
    "write-json": "^0.2.1"
  },
  "devDependencies": {
    "mocha": "*"
  },
  "keywords": [
    "bower.json",
    "package.json",
    "sync"
  ],
  "verb": {
    "related": {
      "list": [
        "load-pkg",
        "filter-object",
        "omit-empty"
      ]
    }
  }
}
